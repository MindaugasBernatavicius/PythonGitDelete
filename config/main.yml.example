application:
  aws_secret_user: <user>
  aws_secret_pass: <pass>
database:
  user: <user>
  pass: <pass>
logging:
  version: 1
  formatters:
    default:
      format: '%(asctime)s %(levelname)-8s [%(name)s] %(message)s'
      datefmt: '%Y-%m-%d %H:%M:%S'
  handlers:
    file:
      class: logging.FileHandler
      level: DEBUG
      formatter: default
      filename: logs/main.log
  loggers:
    main:
      level: DEBUG
      handlers: [ file ]